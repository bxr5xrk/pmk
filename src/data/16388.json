{
  "914942": {
    "questionText": "Коли відбулося становлення біоетики?",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478627",
      "answerText": "70-ті роки ХХ ст"
    }
  },
  "914943": {
    "questionText": "Становлення біоетики відбулося на стику:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478636",
      "answerText": "Всі відповіді вірні"
    }
  },
  "914944": {
    "questionText": "Термін  “біоетика” започаткував:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478637",
      "answerText": "РПоттер"
    }
  },
  "914945": {
    "questionText": "Завданням біоетики є розв’язання етичних проблем, що пов’язані з:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478644",
      "answerText": "Медичною практикою та біомедичними дослідженнями"
    }
  },
  "914946": {
    "questionText": "Консервативну етичну традицію в біомедичній етиці формує вчення:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478651",
      "answerText": "Гедонізм"
    }
  },
  "914947": {
    "questionText": "Ліберальна позиція в медичній етиці базується на:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478654",
      "answerText": "Ученні Ф Ніцше"
    }
  },
  "914948": {
    "questionText": "Хто є автором теорії принципалізму?",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478658",
      "answerText": "Дж Чілдрес"
    }
  },
  "914949": {
    "questionText": "До умовних зобов’язань належать всі, окрім:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478665",
      "answerText": "Зобов’язання вірності"
    }
  },
  "914950": {
    "questionText": "Етика – це наука про:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478668",
      "answerText": "Мораль"
    }
  },
  "914951": {
    "questionText": "Автором «клятви” медичного працівника, яка є основою « Женевської клятви» був:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478675",
      "answerText": "Гіппократ"
    }
  },
  "914952": {
    "questionText": "Біоетика є наукою, що:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478681",
      "answerText": "Усі відповіді вірні"
    }
  },
  "914953": {
    "questionText": "Засновником античної етики є:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478683",
      "answerText": "Сократ"
    }
  },
  "914954": {
    "questionText": "Поняття prima facie  використовується у теорії:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478691",
      "answerText": "ДМілля"
    }
  },
  "914955": {
    "questionText": "Концепцію неоутилітаризму запропонував:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478696",
      "answerText": "І Бентам"
    }
  },
  "914956": {
    "questionText": "Згідно теорії І. Канта знання про етику базуються на:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478700",
      "answerText": "Розумі"
    }
  },
  "914957": {
    "questionText": "Відмінною рисою теорій, заснованих на принципах є:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478703",
      "answerText": "Близькість до загальноприйнятої моралі"
    }
  },
  "914958": {
    "questionText": "За Платоном головною чеснотою є:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478709",
      "answerText": "Справедливість"
    }
  },
  "914959": {
    "questionText": "У працях \"Корпус Гіппократа\" ,\"Про лікаря\", \"Про древню медицину” розглянуті такі найважливіші проблеми:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478716",
      "answerText": "Усі  відповіді вірні"
    }
  },
  "914960": {
    "questionText": "Засновником оригінальної теорії про ноосферу є:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478718",
      "answerText": "В Вернадський"
    }
  },
  "914961": {
    "questionText": "Нооетика має щонайтісніший зв'язок з:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478724",
      "answerText": "Людською свідомістю"
    }
  },
  "914962": {
    "questionText": "Автором етики турботи є:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478729",
      "answerText": "К Гілліган"
    }
  },
  "914963": {
    "questionText": "Теорія доброчесної етики продовжує традиції:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478732",
      "answerText": "Арістотеля"
    }
  },
  "914964": {
    "questionText": "Казуїстичний метод в біомедичному аспекті запропонував:",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478740",
      "answerText": "С Тулмин"
    }
  },
  "914965": {
    "questionText": "Cкільки рівнів в розумовій рівновазі визначає р. Витч?",
    "categoryId": "16388",
    "answer": {
      "answerId": "4478744",
      "answerText": "4"
    }
  }
