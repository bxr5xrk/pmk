{
  "1192465": {
    "questionText": "Основоположником сучасної педагогіки є:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851467",
      "answerText": "ЯА Коменський"
    }
  },
  "1192466": {
    "questionText": "Я.А. Коменський розробив:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851472",
      "answerText": "класно-урочну систему"
    }
  },
  "1192467": {
    "questionText": "Автор праці «Велика дидактика»:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851478",
      "answerText": "ЯА Коменський"
    }
  },
  "1192468": {
    "questionText": "Педагогіка сформувалася як самостійна наука:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851484",
      "answerText": "XVI ст"
    }
  },
  "1192469": {
    "questionText": ": Термін «педагогіка» зародився в:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851487",
      "answerText": "Давній Греції"
    }
  },
  "1192470": {
    "questionText": "У Давній Греції рабів, що супроводжували дітей до школи називали:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851493",
      "answerText": "педагогами"
    }
  },
  "1192471": {
    "questionText": "У період Античності функціонували такі виховні системи:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851499",
      "answerText": "афінська і спартанська"
    }
  },
  "1192472": {
    "questionText": "В епоху Середньовіччя світоглядною основою виховання був:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851503",
      "answerText": "теоцентризм"
    }
  },
  "1192473": {
    "questionText": "Лікар Е. Сеген увійшов в історію педагогіки як:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851508",
      "answerText": "основоположник олігофренопедагогіки"
    }
  },
  "1192474": {
    "questionText": "Для педагогіки періоду Відродження є характерним:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851512",
      "answerText": "гуманістичне спрямування"
    }
  },
  "1192475": {
    "questionText": "Італійська лікар Марія Монтессорі є розробником:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851518",
      "answerText": "концепції всебічного розвитку"
    }
  },
  "1192476": {
    "questionText": "Лікар, автор праці «Як любити дітей?»:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851525",
      "answerText": "Януш Корчак"
    }
  },
  "1192477": {
    "questionText": "В період Середньовіччя функціонували такі типи шкіл:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851527",
      "answerText": "монастирська і лицарська"
    }
  },
  "1192478": {
    "questionText": "Метою афінської системи виховання було:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851535",
      "answerText": "формування гармонійної особистості"
    }
  },
  "1192479": {
    "questionText": "Мета спартанського виховання:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851538",
      "answerText": "виховання воїна"
    }
  },
  "1192480": {
    "questionText": "Основні категорії педагогіки:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851542",
      "answerText": "виховання, навчання, освіта"
    }
  },
  "1192481": {
    "questionText": "Галузь педагогічної науки,  що досліджує закономірності виховання, навчання та освіти різних вікових категорій людей:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851550",
      "answerText": "вікова педагогіка"
    }
  },
  "1192482": {
    "questionText": "Галузь педагогічної науки, що вивчає проблеми організації освітнього процесу осіб з особливими освітніми потребами:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851552",
      "answerText": "корекційна педагогіка"
    }
  },
  "1192483": {
    "questionText": "Розділи загальної педагогіки:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851559",
      "answerText": "загальні основи педагогіки, теорія навчання, теорія виховання, школознавство"
    }
  },
  "1192484": {
    "questionText": "Виховання – це:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851562",
      "answerText": "спеціально організований процес педагогічного впливу на особистість"
    }
  },
  "1192485": {
    "questionText": "Особливості процесу виховання:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851567",
      "answerText": "неперервність, тривалість у часі"
    }
  },
  "1192486": {
    "questionText": "Перший виховний досвід людина отримує в:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851572",
      "answerText": "сім’ї"
    }
  },
  "1192487": {
    "questionText": "Виховним авторитетом дитини дошкільного віку є:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851581",
      "answerText": "батьки"
    }
  },
  "1192488": {
    "questionText": "На виховання особистості в молодшому шкільному віці найбільший вплив має:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851582",
      "answerText": "вчитель"
    }
  },
  "1192489": {
    "questionText": "Специфіка  виховання на всіх історичних етапах визначається:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851589",
      "answerText": "вимогами суспільства і об’єктивними потребами виробництва:"
    }
  },
  "1192490": {
    "questionText": "Ефективність виховання залежить від рівня сформованості:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851592",
      "answerText": "мотивації особистості"
    }
  },
  "1192491": {
    "questionText": "Виховною метою цивілізованого суспільства є:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851600",
      "answerText": "всебічно розвинута і гармонійна особистість"
    }
  },
  "1192492": {
    "questionText": "Виховний ідеал це:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851602",
      "answerText": "уявлення про еталон поведінки і взаємин між людьми"
    }
  },
  "1192493": {
    "questionText": "Український педагог, автор праці «Виховний ідеал»:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851607",
      "answerText": "Г Ващенко"
    }
  },
  "1192494": {
    "questionText": "Національне виховання в українській школі орієнтується на історичні потреби нації, головна серед яких:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851612",
      "answerText": "державотворення"
    }
  },
  "1192495": {
    "questionText": ": Прийоми самовиховання:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851618",
      "answerText": "самопереконання, самонавіювання, самозаохочення, самопримус"
    }
  },
  "1192496": {
    "questionText": "Напрями виховання:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851623",
      "answerText": "розумове, моральне, естетичне, правове, екологічне"
    }
  },
  "1192497": {
    "questionText": "Самовиховання це:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851629",
      "answerText": "управління суб’єктом своєю діяльністю, спілкуванням, поведінкою"
    }
  },
  "1192498": {
    "questionText": "Який принцип виховання означає, що позитивна установка на хороше в людині, довіру до здорових намірів і прагнень учнів дає змогу йому самовиражатися, самоутверджуватися в діяльності, бути відкритим до зовнішніх впливів?",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851634",
      "answerText": "опори на позитивне"
    }
  },
  "1192499": {
    "questionText": "Формувати у вихованців позитивні моральні якості  спроможна людина, яка, насамперед…",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851637",
      "answerText": "сама вихована та володіє цими якостями"
    }
  },
  "1192500": {
    "questionText": "Галузь педагогіки, яка розробляє теорію навчання:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851642",
      "answerText": "дидактика"
    }
  },
  "1192501": {
    "questionText": "З точки зору дидактики учніння це:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851647",
      "answerText": "пізнавальна діяльність учня"
    }
  },
  "1192502": {
    "questionText": "Основні категорії дидактики:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851651",
      "answerText": "навчання, освіта, самоосвіта, викладання, учіння"
    }
  },
  "1192503": {
    "questionText": "Дидактичні закономірності відображають взаємозв’язок між:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851655",
      "answerText": "діяльністю вчителя, діяльністю учнів та змістом навчання"
    }
  },
  "1192504": {
    "questionText": "Принципи навчання це:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851660",
      "answerText": "система основних дидактичних вимог до організації навчання, дотримання яких забезпечує його ефективність"
    }
  },
  "1192505": {
    "questionText": "Принцип систематичності і послідовності навчання забезпечує:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851667",
      "answerText": "послідовність етапів засвоєння знань"
    }
  },
  "1192506": {
    "questionText": "Власна навчальна діяльність студента – це…",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851668",
      "answerText": "учіння"
    }
  },
  "1192507": {
    "questionText": "Автоматизовані, безпомилково виконувані дії, доведені до автоматизму вміння називають…",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851675",
      "answerText": "навичками"
    }
  },
  "1192508": {
    "questionText": "Факти, відомості, наукові теорії, закони, поняття, закріплені у свідомості людини, називають…",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851679",
      "answerText": "знаннями"
    }
  },
  "1192509": {
    "questionText": "Згідно з принципом доступності, навчальний матеріал:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851685",
      "answerText": "ускладнюється в результаті накопичення досвіду"
    }
  },
  "1192510": {
    "questionText": "До головних видів знань належать:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851688",
      "answerText": "основні терміни і поняття"
    }
  },
  "1192511": {
    "questionText": "Процес взаємодії викладача та студента, в результаті якого студент засвоює знання, набуває вмінь і навичок – це:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851694",
      "answerText": "навчання"
    }
  },
  "1192512": {
    "questionText": "Принцип наочності базується на:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851698",
      "answerText": "провідному значенні зорового  аналізатора  у сприйманні зовнішнього світу"
    }
  },
  "1192513": {
    "questionText": "У якій послідовності набуваються та формуються елементи навчання?",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851703",
      "answerText": "знання, уміння, навички"
    }
  },
  "1192514": {
    "questionText": "Назвати види контролю результатів навчання:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851710",
      "answerText": "попередній, поточний, тематичний, підсумковий"
    }
  },
  "1192515": {
    "questionText": "Завдання дидактики:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851713",
      "answerText": "удосконалення форм і методів навчання, розробка навчальних технологій"
    }
  },
  "1192516": {
    "questionText": "Предметом дидактики є:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851720",
      "answerText": "процес навчання"
    }
  },
  "1192517": {
    "questionText": "Показниками якості знань є:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851723",
      "answerText": "повнота, системність, дієвість, міцність"
    }
  },
  "1192518": {
    "questionText": "Навички – це:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851729",
      "answerText": "уміння, доведені до автоматизму"
    }
  },
  "1192519": {
    "questionText": "Здатність людини свідомо діяти на основі засвоєних знань це:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851735",
      "answerText": "уміння"
    }
  },
  "1192520": {
    "questionText": "Процес навчання включає:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851738",
      "answerText": "діяльність педагога – викладання та діяльність студентів – учіння"
    }
  },
  "1192521": {
    "questionText": "Принципи навчання – це:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851743",
      "answerText": "вихідні положення організації навчання"
    }
  },
  "1192522": {
    "questionText": "Від чого залежить кінцевий результат процесу навчання?:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851749",
      "answerText": "від функціонування педагогічного процесу як системи"
    }
  },
  "1192523": {
    "questionText": "Дидактичні принципи забезпечують ефективність, коли застосовуються:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851755",
      "answerText": "комплексно"
    }
  },
  "1192524": {
    "questionText": "Узагальнена характеристика навчальних систем, що встановлює особливості учіння  та викладання це:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851758",
      "answerText": "вид навчання"
    }
  },
  "1192525": {
    "questionText": "Види навчання класифікують за:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851763",
      "answerText": "змістом, кількістю учнів, формами передачі знань"
    }
  },
  "1192526": {
    "questionText": "Для традиційних видів  навчання характерною є:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851766",
      "answerText": "відсутність умов для самостійної пізнавальної діяльності"
    }
  },
  "1192527": {
    "questionText": "Позиція здобувача освіти при традиційних видах навчання:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851771",
      "answerText": "підпорядкований об’єкт навчальних впливів"
    }
  },
  "1192528": {
    "questionText": "Позиція викладача  при традиційних видах навчання:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851774",
      "answerText": "головна, єдина ініціативна особа"
    }
  },
  "1192529": {
    "questionText": "Позиція здобувача освіти при сучасних  видах навчання:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851780",
      "answerText": "активний суб’єкт навчального процесу"
    }
  },
  "1192530": {
    "questionText": "Позиція викладача  при сучасних  видах навчання:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851785",
      "answerText": "не єдина ініціативна особа"
    }
  },
  "1192531": {
    "questionText": "Процес виявлення цілей особистості та  вироблення оптимальних шляхів їх досягнення це:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851787",
      "answerText": "коучинг"
    }
  },
  "1192532": {
    "questionText": "У дослівному перекладі слово «коуч» означає:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851790",
      "answerText": "тренер, наставник"
    }
  },
  "1192533": {
    "questionText": "Першим етапом коучингу є:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851795",
      "answerText": "постановка мети"
    }
  },
  "1192534": {
    "questionText": "Завдяки допомозі коуча особа здатна :",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851799",
      "answerText": "вирішувати свої  завдання самостійно"
    }
  },
  "1192535": {
    "questionText": "У сучасній  українській освіті коучинг набув  розповсюдження завдяки;",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851804",
      "answerText": "впровадженню педагогіки партнерства"
    }
  },
  "1192536": {
    "questionText": "Фасилітатор це:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851808",
      "answerText": "особа, яка забезпечує успішну групову комунікацію"
    }
  },
  "1192537": {
    "questionText": "До кола повноважень  фасилітатора  входить:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851812",
      "answerText": "контроль за  дотриманням регламенту, правил, процедури колективної зустрічі"
    }
  },
  "1192538": {
    "questionText": "Заохочення учнів до ефективного колективного спілкування здійснює:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851814",
      "answerText": "фасилітатор:"
    }
  },
  "1192539": {
    "questionText": ".  Методи засвоєння знань при традиційному виді навчання грунтуються на:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851818",
      "answerText": "повідомленні готових знань"
    }
  },
  "1192540": {
    "questionText": "Завдання інтегративного навчання:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851822",
      "answerText": "усунення протиріч між невпинно зростаючим обсягом знань та здатністю їх засвоєння"
    }
  },
  "1192541": {
    "questionText": "Формою інтегрованого навчання є:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851827",
      "answerText": "проектна робота"
    }
  },
  "1192542": {
    "questionText": "Основою проблемно-розвиваючого навчання є:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851831",
      "answerText": "створення проблемної ситуації"
    }
  },
  "1192543": {
    "questionText": "Слово «евристика» в перекладі з грецької мови означає:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851834",
      "answerText": "відкриття"
    }
  },
  "1192544": {
    "questionText": "Евристичне навчання пропонує студентам:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851840",
      "answerText": "самостійно відкривати знання"
    }
  },
  "1192545": {
    "questionText": "Основоположником евристичного навчання вважається:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851844",
      "answerText": "Сократ"
    }
  },
  "1192546": {
    "questionText": "Методи інтерактивного навчання:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851846",
      "answerText": "мозковий штурм"
    }
  },
  "1192547": {
    "questionText": "Одне із домінуючих вмінь педагога – володіння мистецтвом живого слова, виокремив у своїх працях:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851850",
      "answerText": "КУшинський"
    }
  },
  "1192548": {
    "questionText": "А.Макаренко рекомендував у педагогічних вузах ввести на заняттях з риторики:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851856",
      "answerText": "роботу над постановкою голосу"
    }
  },
  "1192549": {
    "questionText": "Вислів «Шукайте цензора в собі» належить:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851858",
      "answerText": "ЛКостенко"
    }
  },
  "1192550": {
    "questionText": "Критерієм правильності мовлення є:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851863",
      "answerText": "мовна дійсність конкретної епохи"
    }
  },
  "1192551": {
    "questionText": "Які компоненти включає структура професійно-мовленнєвої готовності майбутнього викладача:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851869",
      "answerText": "методи, прийоми, закономірності, принципи, засоби, форми"
    }
  },
  "1192552": {
    "questionText": "Професійно-мовленнєва діяльність – це:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851870",
      "answerText": "система сформованих професійних мовленнєвих знань, умінь і навичок та комплекс індивідуально-психологічних властивостей особистості"
    }
  },
  "1192553": {
    "questionText": "Для оволодіння належною технікою мовлення, необхідно:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851876",
      "answerText": "володіти диханням, керувати голосом, виробити навички артикуляції, засвоїти орфоепічні норми"
    }
  },
  "1192554": {
    "questionText": "Дикція – це:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851879",
      "answerText": "правильна, виразна артикуляція, тобто робота мовних органів, спрямована на вимову звуків"
    }
  },
  "1192555": {
    "questionText": "Сила голосу залежить від привальної координації двох складових голосотворення:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851884",
      "answerText": "дихання, артикуляції"
    }
  },
  "1192556": {
    "questionText": "Засоби виразності усного мовлення становлять:",
    "categoryId": "23219",
    "answer": {
      "answerId": "5851887",
      "answerText": "інтонацію"
    }
  }
}